{
	"expo": {
		"name": "Yarr - Bourbon Island's Pirates",
		"slug": "bourbonisland",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "bourbonisland",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#4c2818"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": false,
			"infoPlist": {
				"NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera"
			},
			"bundleIdentifier": "be.henallux.masi.christophebernard.bourbonisland"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#4c2818"
			},
			"permissions": ["android.permission.CAMERA"],
			"package": "be.henallux.masi.christophebernard.bourbonisland"
		},
		"plugins": [
			"expo-router",
			[
				"expo-camera",
				{
					"cameraPermission": "Allow $(PRODUCT_NAME) to access your camera.",
					"recordAudioAndroid": false
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}
